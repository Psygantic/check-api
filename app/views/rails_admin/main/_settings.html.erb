<% serialized_field = @object.respond_to?(:settings) ? @object.settings : @object.data %>
<% if serialized_field %>
  <% serialized_field.each do |key, value| %>
    <%= label :project, "settings[#{key.to_sym}]", key %>
    <%= text_field :project, "settings[#{key.to_sym}]", value: @object.send(key.to_sym) %>
  <% end %>
<% end %>
